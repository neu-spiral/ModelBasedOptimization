#!/bin/bash




#network model
net_model=ConvAEC2

#network dimensions
m_dim=1
m_prime=16





batch=8

for data in MNIST_CIFAR10outliers00 MNIST_CIFAR10outliers005 MNIST_CIFAR10outliers01  
do
    for p in  1 1.5 2 -2
    do
        for lr in  0.000001
         
        do
            for momentum in 0.9 
            do
                sbatch  -p ioannidis --mem 200g --job-name SGD$data  --time 6-00:00:00 MBO_script $data SGD  $net_model $m_dim $m_prime $batch $p $lr $momentum
            done
        done
    done

done




