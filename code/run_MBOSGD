#!/bin/bash




#network model
net_model=Linear

#network dimensions
m_dim=280
m_prime=16






batch=8


for data in MTR_scm1doutliers00 MTR_scm1doutliers005 MTR_scm1doutliers01
do
    for p in 1 1.5  2
    do
        for lr in 0.001 
        do
             for momentum in 0.9
             do
                 sbatch -p ioannidis --mem 200g  --job-name MBO2$data --time 6-00:00:00 MBOSGD_script $data   $net_model $m_dim $m_prime $batch $p $lr $momentum
             done
        done
    done
done




