#!/bin/bash




#network model
net_model=ConvAEC2Soft

#network dimensions
m_dim=1
m_prime=8





lr=0.001
momentum=0.0


batch=8

#dataset
for data in  MNIST_dc1.5__Fakeoutliers00 MNIST_dc1.5__Fakeoutliers005 MNIST_dc1.5__Fakeoutliers01 MNIST_dc1.5__Fakeoutliers02 MNIST_dc1.5__Fakeoutliers03
do

    for p in  1 1.5 2
    do
         sbatch --partition ioannidis  --mem 100g --constraint E5-2680v4@2.40GHz   --job-name $data  MBO_script $data MBO  $net_model $m_dim $m_prime $batch $p $lr $momentum
    done
done




