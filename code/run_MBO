#!/bin/bash


#dataset
data=MTR_scm1d_outliers00


#network model
net_model=Linear

#network dimensions
m_dim=280
m_prime=16




job_name=$net_model

lr=0.001
momentum=0.0


batch=8

for data in MTR_scm1d_outliers00 MTR_scm1d_outliers005 MTR_scm1d_outliers01
do

    for p in 1 1.5 2
    do
         sbatch -p ioannidis --mem 100g  --job-name $job_name MBO_script $data MBO  $net_model $m_dim $m_prime $batch $p $lr $momentum
    done
done




